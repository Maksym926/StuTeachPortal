<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Text layoutX="14.0" layoutY="52.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Edit Personal Info">
         <font>
            <Font size="22.0" />
         </font>
      </Text>
      <Text layoutX="14.0" layoutY="87.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Name" />
      <TextField fx:id="nameField" layoutX="14.0" layoutY="98.0" />
      <Text layoutX="14.0" layoutY="145.0" strokeType="OUTSIDE" strokeWidth="0.0" text="City" />
      <TextField fx:id="cityField" layoutX="14.0" layoutY="156.0" />
      <Text layoutX="14.0" layoutY="203.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Email" />
      <TextField fx:id="emailField" layoutX="14.0" layoutY="214.0" />
      <Button layoutX="17.0" layoutY="255.0" mnemonicParsing="false" onAction="#submitChanges" text="Submit Changes " />
      <Button onAction="#returnToPreviousForm">
         <graphic>
            <ImageView fitHeight="20" fitWidth="20">
               <image>
                  <Image url="@/images/arrow_back.png"/>
               </image>
            </ImageView>
         </graphic>
      </Button>
   </children>
</AnchorPane>
