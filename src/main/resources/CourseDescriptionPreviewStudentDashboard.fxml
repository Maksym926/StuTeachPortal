      </Text>
      <Text fx:id="courseCode" layoutX="23.0" layoutY="80.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Course code" />
      <Text fx:id="courseInstructor" layoutX="23.0" layoutY="108.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Instructor" />
      <Text fx:id="courseDuration" layoutX="23.0" layoutY="138.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Course duration" />
