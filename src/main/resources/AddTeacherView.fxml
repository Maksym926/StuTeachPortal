      <PasswordField fx:id="password" layoutX="68.0" layoutY="235.0" />
      <Text layoutX="68.0" layoutY="231.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Password" />
      <Text layoutX="68.0" layoutY="288.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Course" />
      <Button layoutX="67.0" layoutY="343.0" mnemonicParsing="false" onAction="#addTeacher" text="Add teacher" />
